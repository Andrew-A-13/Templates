struct pref {
    vector<int> p;
    int n;
    pref(vector<int>& A)
    {
        n = A.size();
        p.resize(n);
        p[0] = A[0];
        for (int i = 1; i < n; i++)p[i] = p[i - 1] + A[i];
    }
    int get(int l, int r)
    {
        return (r != 0 ? p[r - 1] : 0LL) - (l != 0 ? p[l - 1] : 0LL);
    }
};
